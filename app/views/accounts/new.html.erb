<h2>Sign Up</h2>
<%= form_for(@account) do |account|%>
	<p>
		<%= account.label :name %>
		<br>
		<%= account.text_field :name %>
	</p>
	<%= account.field_for :owner do |owner|%>
	<p> <%=owner.label :email %>
		<%=owner.email_field :email %>
	</p>
	<p> <%=owner.label :password %>
		<%=owner.password_field :password %>
	</p>
	<%end%>
		<%= account.submit %>
<%end%>
