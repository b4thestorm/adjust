<h1>Register</h1>

<%=form_for @employee do |f| %>
<%=f.label :username %>
<%=f.text_field :username %>
</br>
<%=f.label :FirstName%>
<%=f.text_field :f_name%>
</br>
<%=f.label :Lastname%>
<%=f.text_field :l_name%>
</br>
<%=f.label :email%>
<%=f.text_field :email%>
</br>
<%=f.label :password%>
<%=f.password_field :password%>
</br>
<%=f.submit ('Register')%>
<%end%>
